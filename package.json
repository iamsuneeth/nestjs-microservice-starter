{
  "name": "gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "sh ./scripts/typeorm.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/service-gateway/test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.3.7",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.9",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.0.0",
    "@sendgrid/mail": "^7.1.0",
    "@types/auth0": "^2.20.5",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.14.2",
    "asyncctx": "^2.0.12",
    "auth0": "^2.25.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connect-redis": "^4.0.4",
    "express-session": "^1.17.1",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "grpc": "^1.24.2",
    "infra": "^1.0.3",
    "ioredis": "^4.16.3",
    "jwks-rsa": "^1.8.0",
    "mailgen": "^2.0.10",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.3",
    "proto": "^1.0.19",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "ts-proto": "^1.20.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/faker": "^4.1.11",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "25.1.4",
    "@types/mailgen": "^2.0.3",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-auth0": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^6.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/infra/(.*)": "<rootDir>/libs/infra/src/$1",
      "@app/infra": "<rootDir>/libs/infra/src",
      "@app/proto/(.*)": "<rootDir>/libs/proto/src/$1",
      "@app/proto": "<rootDir>/libs/proto/src",
      "@app/grpc/(.*)": "<rootDir>/libs/grpc/src/$1",
      "@app/grpc": "<rootDir>/libs/grpc/src",
      "@app/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@app/common": "<rootDir>/libs/common/src",
      "@app/orm/(.*)": "<rootDir>/libs/orm/src/$1",
      "@app/orm": "<rootDir>/libs/orm/src",
      "@app/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@app/db": "<rootDir>/libs/db/src"
    }
  }
}
